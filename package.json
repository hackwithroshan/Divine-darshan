{
  "name": "astrologica-frontend",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "npm install --prefix backend",
    "dev": "npx concurrently --kill-others-on-fail --names \"FRONTEND,BACKEND\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm:dev:frontend\" \"npm:dev:backend\"",
    "dev:frontend": "node dev.js",
    "dev:backend": "npm run dev:start --prefix backend",
    "build": "node build.js",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "esbuild": "latest",
    "fs-extra": "^11.2.0",
    "lucide-react": "latest",
    "react": "latest",
    "react-dom": "latest",
    "recharts": "latest"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "concurrently": "^8.2.2"
  }
}